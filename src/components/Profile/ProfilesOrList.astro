---
import List from "./List.astro";
import Profile from "./Profile.astro";
import type { ChairsInterface } from "./profiles";
import Profiles from "./Profiles.astro";

const chair = Astro.props as ChairsInterface;
---

{
  chair.roll === "Area Chairs" ? (
    <List profiles={chair.profiles} />
  ) : (
    <Profiles profiles={chair.profiles} />
  )
}
